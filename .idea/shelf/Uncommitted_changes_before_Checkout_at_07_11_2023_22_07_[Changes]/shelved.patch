<+>import org.junit.jupiter.api.Test;\r\nimport static org.junit.jupiter.api.Assertions.*;\r\n\r\npublic class Matrix_Test {\r\n\r\n    @Test\r\n    void testEmptyMatrixCreation() {\r\n        ImmutableMatrix matrix = new ImmutableMatrix();\r\n        assertEquals(0, matrix.getDimensions()[0]);\r\n        assertEquals(0, matrix.getDimensions()[1]);\r\n    }\r\n\r\n    @Test\r\n    void testMatrixCreationWithDimensions() {\r\n        int rows = 3;\r\n        int cols = 4;\r\n        ImmutableMatrix matrix = new ImmutableMatrix(rows, cols);\r\n        assertEquals(rows, matrix.getDimensions()[0]);\r\n        assertEquals(cols, matrix.getDimensions()[1]);\r\n    }\r\n\r\n    @Test\r\n    void testMatrixCopyConstructor() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}};\r\n        ImmutableMatrix originalMatrix = new ImmutableMatrix(2, 3).fillMatrix(values);\r\n        ImmutableMatrix copiedMatrix = new ImmutableMatrix(originalMatrix);\r\n\r\n        assertEquals(originalMatrix, copiedMatrix);\r\n        assertNotSame(originalMatrix, copiedMatrix); // Ensure a deep copy\r\n    }\r\n\r\n    @Test\r\n    void testFillMatrix() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(2, 3).fillMatrix(values);\r\n\r\n        for (int i = 0; i < 2; i++) {\r\n            assertArrayEquals(values[i], matrix.getRow(i));\r\n        }\r\n    }\r\n\r\n    @Test\r\n    void testGetElement() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(2, 3).fillMatrix(values);\r\n\r\n        assertEquals(2, matrix.getElement(0, 1));\r\n        assertEquals(4, matrix.getElement(1, 0));\r\n    }\r\n\r\n    @Test\r\n    void testGetRow() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(2, 3).fillMatrix(values);\r\n\r\n        assertArrayEquals(values[1], matrix.getRow(1));\r\n    }\r\n\r\n    @Test\r\n    void testGetColumn() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(2, 3).fillMatrix(values);\r\n\r\n        int[] expectedColumn = {2, 5};\r\n        assertArrayEquals(expectedColumn, matrix.getColumn(1));\r\n    }\r\n\r\n    @Test\r\n    void testEquality() {\r\n        int[][] values1 = {{1, 2}, {3, 4}};\r\n        int[][] values2 = {{1, 2}, {3, 4}};\r\n\r\n        ImmutableMatrix matrix1 = new ImmutableMatrix(2, 2).fillMatrix(values1);\r\n        ImmutableMatrix matrix2 = new ImmutableMatrix(2, 2).fillMatrix(values2);\r\n\r\n        assertEquals(matrix1, matrix2);\r\n    }\r\n\r\n    @Test\r\n    void testInequality() {\r\n        int[][] values1 = {{1, 2}, {3, 4}};\r\n        int[][] values2 = {{5, 6}, {7, 8}};\r\n\r\n        ImmutableMatrix matrix1 = new ImmutableMatrix(2, 2).fillMatrix(values1);\r\n        ImmutableMatrix matrix2 = new ImmutableMatrix(2, 2).fillMatrix(values2);\r\n\r\n        assertNotEquals(matrix1, matrix2);\r\n    }\r\n    @Test\r\n    void testImmutability() {\r\n        int[][] values = {{1, 2}, {3, 4}};\r\n        ImmutableMatrix originalMatrix = new ImmutableMatrix(2, 2).fillMatrix(values);\r\n        ImmutableMatrix modifiedMatrix = originalMatrix.fillMatrix(new int[][]{{5, 6}, {7, 8}});\r\n\r\n        // Ensure the original matrix remains unchanged\r\n        assertNotEquals(originalMatrix, modifiedMatrix);\r\n        assertArrayEquals(values[0], originalMatrix.getRow(0));\r\n        assertArrayEquals(values[1], originalMatrix.getRow(1));\r\n    }\r\n    @Test\r\n    void testMatrixAddition() {\r\n        int[][] values1 = {{1, 2}, {3, 4}};\r\n        int[][] values2 = {{5, 6}, {7, 8}};\r\n\r\n        ImmutableMatrix matrix1 = new ImmutableMatrix(2, 2).fillMatrix(values1);\r\n        ImmutableMatrix matrix2 = new ImmutableMatrix(2, 2).fillMatrix(values2);\r\n        ImmutableMatrix resultMatrix = matrix1.add(matrix2);\r\n\r\n        int[][] expectedValues = {{6, 8}, {10, 12}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(2, 2).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, resultMatrix);\r\n    }\r\n\r\n    @Test\r\n    void testScalarMultiplication() {\r\n        int[][] values = {{1, 2}, {3, 4}};\r\n        ImmutableMatrix originalMatrix = new ImmutableMatrix(2, 2).fillMatrix(values);\r\n        int scalar = 3;\r\n        ImmutableMatrix resultMatrix = originalMatrix.multiplyScalar(scalar);\r\n\r\n        int[][] expectedValues = {{3, 6}, {9, 12}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(2, 2).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, resultMatrix);\r\n    }\r\n    @Test\r\n    void testMatrixMultiplication() {\r\n        int[][] values1 = {{1, 2}, {3, 4}};\r\n        int[][] values2 = {{5, 6}, {7, 8}};\r\n\r\n        ImmutableMatrix matrix1 = new ImmutableMatrix(2, 2).fillMatrix(values1);\r\n        ImmutableMatrix matrix2 = new ImmutableMatrix(2, 2).fillMatrix(values2);\r\n        ImmutableMatrix resultMatrix = matrix1.multiply(matrix2);\r\n\r\n        int[][] expectedValues = {{19, 22}, {43, 50}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(2, 2).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, resultMatrix);\r\n    }\r\n\r\n    @Test\r\n    void testInvalidMatrixMultiplicationDimensions() {\r\n        int[][] values1 = {{1, 2}, {3, 4}};\r\n        int[][] values2 = {{5, 6}, {7, 8},{9,10}};\r\n\r\n        ImmutableMatrix matrix1 = new ImmutableMatrix(2, 2).fillMatrix(values1);\r\n        ImmutableMatrix matrix2 = new ImmutableMatrix(3, 2).fillMatrix(values2);\r\n\r\n        assertThrows(IllegalArgumentException.class, () -> matrix1.multiply(matrix2));\r\n    }\r\n    @Test\r\n    void testMatrixTranspose() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(2, 3).fillMatrix(values);\r\n        ImmutableMatrix transposedMatrix = matrix.transpose();\r\n\r\n        int[][] expectedValues = {{1, 4}, {2, 5}, {3, 6}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(3, 2).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, transposedMatrix);\r\n    }\r\n    @Test\r\n    void testDiagonalMatrixCreation() {\r\n        int[] diagonalValues = {1, 2, 3};\r\n        ImmutableMatrix diagonalMatrix = ImmutableMatrix.DiagonalMatrix(diagonalValues);\r\n\r\n        int[][] expectedValues = {{1, 0, 0}, {0, 2, 0}, {0, 0, 3}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(3, 3).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, diagonalMatrix);\r\n    }\r\n    @Test\r\n    void testIdentityMatrixCreation() {\r\n        ImmutableMatrix IdentityMatrix = ImmutableMatrix.IdentityMatrix(3);\r\n\r\n        int[][] expectedValues = {{1, 0, 0}, {0, 1, 0}, {0, 0, 1}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(3, 3).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, IdentityMatrix);\r\n    }\r\n    @Test\r\n    void testUpperTriangularMatrix() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(3, 3).fillMatrix(values);\r\n        ImmutableMatrix upperTriangularMatrix = matrix.UpperTriangularMatrix();\r\n\r\n        int[][] expectedValues = {{1, 2, 3}, {0, 5, 6}, {0, 0, 9}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(3, 3).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, upperTriangularMatrix);\r\n    }\r\n\r\n    @Test\r\n    void testLowerTriangularMatrix() {\r\n        int[][] values = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};\r\n        ImmutableMatrix matrix = new ImmutableMatrix(3, 3).fillMatrix(values);\r\n        ImmutableMatrix lowerTriangularMatrix = matrix.LowerTriangularMatrix();\r\n\r\n        int[][] expectedValues = {{1, 0, 0}, {4, 5, 0}, {7, 8, 9}};\r\n        ImmutableMatrix expectedMatrix = new ImmutableMatrix(3, 3).fillMatrix(expectedValues);\r\n\r\n        assertEquals(expectedMatrix, lowerTriangularMatrix);\r\n    }\r\n\r\n}\r\n
